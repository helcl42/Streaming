${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/theplayerserver: ${OBJECTFILES}
	${MKDIR} -p ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}
	${LINK.cc} -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/theplayerserver ${OBJECTFILES} ${LDLIBSOPTIONS} 

